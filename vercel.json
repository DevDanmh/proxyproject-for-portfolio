{
  "rewrites": [
    {
      "source": "/flight-tracker/:match*",
      "destination": "https://airportflights-app.vercel.app/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    },
    {
      "source": "/blog/:match*",
      "destination": "https://dan-portfolio-six.vercel.app/index.html"
    },
    {
      "source": "/:match*",
      "destination": "https://dan-portfolio-six.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://dan-portfolio-six.vercel.app"
    }
  ]
}
